{
  "extends": [
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "rules" : {
    "react/jsx-filename-extension": 0,
    "no-console": 1,
    "react/no-array-index-key": 1,
    "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
    "react/forbid-prop-types": 1,
    "jsx-a11y/label-has-for": 1
  }
}